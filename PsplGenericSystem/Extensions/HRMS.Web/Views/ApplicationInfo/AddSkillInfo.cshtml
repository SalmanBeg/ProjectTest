@model HRMS.Web.ViewModels.ApplicationInfoFormModel
@{
    ViewBag.Title = "AddSkillInfo";
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}

<h3>AddSkillInfo</h3>
@using (Html.BeginForm("AddSkillInfo", "ApplicationInfo", FormMethod.Post))
{
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-md-6">
                <div class="form-group">
                    @Html.LabelFor(k => k.ApplicantAchievementsAndAssociations.Type, new { @class = "col-sm-3 control-label required" })
                    <div class="col-sm-9">
                        @Html.TextBoxFor(k => k.ApplicantAchievementsAndAssociations.Type, new { @class = "form-control" })
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(k => k.ApplicantAchievementsAndAssociations.Description, new { @class = "col-sm-3 control-label " })
                    <div class="col-sm-9">
                        @Html.TextBoxFor(k => k.ApplicantAchievementsAndAssociations.Description, new { @class = "form-control" })
                    </div>
                </div>
                

            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 control-button">
            
            @Html.ActionLink("Cancel", "AddApplicationInfo", null, new { @class = "btn btn-success" })
            <button type="submit" class="btn btn-success">Save</button>
        </div>
    </div>
}







