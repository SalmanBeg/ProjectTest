                                    </td>
                                    <td width="74" align="left" valign="middle" style="padding-left: 2px;">
                                        <img id="iBtnExit" src="~/images/btn-Exit.png" />
                                    </td>
                                    <td></td>
                                </tr>
                            </table>
                        </div>
                    </div>
                    <input type="hidden" id="hfcanvas" />
                    <input type="hidden" id="hfsigdiv" />
                    <input type="hidden" id="hfSignature" />
                </td>
            </tr>
        </table>
    </form>
}
<script>
    /* to display sign tabs */
    
    $('ul.tabs').each(function () {
        // For each set of tabs, we want to keep track of
        // which tab is active and it's associated content
        var $active, $content, $links = $(this).find('a');

        // If the location.hash matches one of the links, use that as the active tab.
        // If no match is found, use the first link as the initial active tab.
        $active = $($links.filter('[href="' + location.hash + '"]')[0] || $links[0]);
        $active.addClass('active');
        activeTab = $active[0].hash;
        $content = $($active[0].hash);
        // Hide the remaining content
        $links.not($active).each(function () {
            $(this.hash).hide();
        });
        // Bind the click event handler
        $(this).on('click', 'a', function (e) {
            // Make the old tab inactive.
            $active.removeClass('active');
            $content.hide();
            // Update the variables with the new link and content
            $active = $(this);
            activeTab = $(this.hash);
            $content = $(this.hash);
            // Make the tab active.
            $active.addClass('active');
            $content.show();
            // Prevent the anchor's default click action
            e.preventDefault();
        });
    });
    $('.sigPad').signaturePad();
   
    /* put border around selected sign */
    var selectedImageId;
    $("input:image").click(function () {
        $("input:image").css("border", '0px');
        $(this).css("border", '3px solid black');
        selectedImageId = $(this).attr('id');
    });
   
    /* when unsign button clicked */
    $('#aUnSignI9Form').click(function (e) {
        e.stopPropagation();
        e.preventDefault();
        UnSignI9Form();
    });
    /* hide sign and update sign id to 0 */
    function UnSignI9Form() {
        $("#ii9sign").hide();
        $("#txtSignDate").attr("value", "");
        var baseurl = '@System.Configuration.ConfigurationManager.AppSettings["BaseURL"]';
        $.ajax({
            url: baseurl + "I9WorkAuthorization/setTempData",
            type: 'post',
            cache: false,
            data: { signId: 0 },
            success: function () {
                var result = $('#btnI9FormSave').click();
                location.reload();
            }
        });
    }
    /* when sign button clicked */
    $('#aSignI9Form').click(function (e) {
        e.stopPropagation();
        e.preventDefault();
        SignI9Form();
    });
    function SignI9Form() {

        if (activeTab != null && activeTab == "#tab1") {
            var isign = $("#ii9sign");
            if (isign.length > 0) {
                isign[0].src = $('#' + selectedImageId).attr('src');
            }
            $("#ii9sign").show();
            var d = new Date();
            var date = (d.getUTCMonth() + 1) + '/' + (d.getUTCDate()) + '/' + (d.getUTCFullYear());
            $("#txtSignDate").attr("value", date);

            /* if existing signature is selected */
            var baseurl = '@System.Configuration.ConfigurationManager.AppSettings["BaseURL"]';
            if (selectedImageId) {
                $.ajax({
                    url: baseurl + "I9WorkAuthorization/setTempData",
                    type: 'post',
                    cache: false,
                    data: { signId: selectedImageId },
                    success: function () {
                        var result = $('#btnI9FormSave').click();
                        location.reload();
                    }
                });
            }
        }
            /* if new signature is signed */
        else {
            var typeItval = $('#EsignType');
            var drawItval = $('#EsignDraw');
            var baseurl = '@System.Configuration.ConfigurationManager.AppSettings["BaseURL"]';
            var canvasstring;
            if (drawItval[0].style.display == "block") {
                var drawingCanvasDraw = document.getElementById('drawingCanvas'),
                    drawingContext = drawingCanvasDraw.getContext('2d');
                var dataUrlLocal;
                dataUrlLocal = drawingCanvasDraw.toDataURL();
                var myImageDraw = drawingCanvasDraw.toDataURL("image/png");
                //myImageDraw = myImageDraw.replace(/^data:image\/(png|jpg);base64,/, "")
                canvasstring = $('#hfcanvas')[0].value;
                var isign = $("#ii9sign");
                if (isign.length > 0) {
                    isign[0].src = myImageDraw;
                } else {
                    alert('please sign.');
                }
                $("#ii9sign").show();
                if ($('#hfcanvas')[0].value == dataUrlLocal || canvasstring == dataUrlLocal) {
                    $("#ii9sign").hide();
                    alert('please sign.');
                    return false;
                }
                myImageDraw = myImageDraw.replace(/^data:image\/(png|jpg);base64,/, "");
                var parms = { fileName: 'I9WorkAuthorizationForm', eSign: myImageDraw, filePath: $("#ii9sign")[0].src };
                $.ajax({
                    url: baseurl + "Files/SignI9Form",
                    type: 'post',
                    cache: false,
                    data: parms,
                    success: function (result) {
                        if (result) {
                            $.ajax({
                                url: baseurl + "I9WorkAuthorization/setTempData",
                                type: 'post',
                                cache: false,
                                data: { signId: result },
                                success: function () {
                                    $('#btnI9FormSave').click();
                                    location.reload();
                                }
                            });
                        }
                    },
                    error: function () {
                        alert(Error);
                    }
                });
            }
            else {

                html2canvas($("#idtyped"), {
                    onrendered: function (canvas) {
                        canvas.id = "idcanvasSubmit";
                        theCanvas = canvas;
                        document.body.appendChild(canvas);
                        var drawingCanvasType = document.getElementById('idcanvasSubmit');
                        var drawingContext = drawingCanvasType.getContext('2d');
                        //drawingContext.clearRect(0, 0, canvas.width, canvas.height);
                        var myImageType = drawingCanvasType.toDataURL("image/png");
                        //myImageType = myImageType.replace(/^data:image\/(png|jpg);base64,/, "");
                        var DefaultDivImage = $('#hfsigdiv')[0].value;
                        var typevalue = $('#name');
                        var isign = $("#ii9sign");
                        if (isign.length > 0) {
                            isign[0].src = myImageType;
                            $("#ii9sign").show();
                        }
                        if (typevalue[0].value == "" || myImageType == DefaultDivImage) {
                            document.body.removeChild(canvas);
                            $("#ii9sign").hide();
                            alert('please sign the signature.');
                            return false;
                        }

                        myImageType = myImageType.replace(/^data:image\/(png|jpg);base64,/, "");
                        var parms = { fileName: 'I9WorkAuthorizationForm', eSign: myImageType, filePath: $("#ii9sign")[0].src };
                        $.ajax({
                            url: baseurl + "Files/SignI9Form",
                            type: 'post',
                            cache: false,
                            data: parms,
                            success: function (result) {
                                if (result) {
                                    $.ajax({
                                        url: baseurl + "I9WorkAuthorization/setTempData",
                                        type: 'post',
                                        cache: false,
                                        data: { signId: result },
                                        success: function () {
                                            $('#btnI9FormSave').click();
                                            location.reload();
                                        }
                                    });
                                }
                            },
                            error: function () {
                                alert(Error);
                            }
                        });
                    }
                });
            }

        }


    }
</script>
